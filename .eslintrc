{
  "extends": "airbnb-base",
  "rules": {
    "no-console": "off",
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "quotes": ["error", "double"],
    "comma-dangle": ["error", "only-multiline"],
    "linebreak-style": ["error", "windows"],
    "function-paren-newline": ["warn", "multiline"]
  }
}
